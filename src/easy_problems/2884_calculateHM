//문제번호 : 2884(입력받은 시간으로부터 45분 빼서 알람 맞추기)

import java.util.Scanner;

public class Main {
    public static void main (String args[]) {
        Scanner s = new Scanner(System.in);
        int H = s.nextInt();
        int M = s.nextInt();
        
        M -= 45;
        
        if (M < 0) {
            H -= 1;
            M += 60;
            if(H < 0) H = 23;
        }
        System.out.println(H + " " + M);
    }
}
