//문제번호 : 11382(long자료형)

import java.util.Scanner;

public class Main {
    public static void main(String args[]) {
        Scanner s = new Scanner(System.in);
        long A = s.nextLong();
        long B = s.nextLong();
        long C = s.nextLong();
        
        System.out.println(A+B+C);
        
        s.close();
        
    }
}
